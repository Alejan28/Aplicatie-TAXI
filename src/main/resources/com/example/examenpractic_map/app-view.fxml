<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.examenpractic_map.Controller.AppController"
            prefHeight="400.0" prefWidth="600.0">
    <ImageView fx:id="imageView" fitHeight="200" fitWidth="200" layoutX="100" layoutY="100" preserveRatio="true" />
    <VBox spacing="10.0">
        <HBox spacing="10.0">
            <Label text="ADRESA DE PLECARE"/>
            <TextField fx:id="plecare"/>
        </HBox>
        <HBox spacing="10.0">
            <Label text="ADRESA DE DESTINATIE"/>
            <TextField fx:id="destinatie"/>
        </HBox>
        <HBox spacing="10.0">
            <Label text="NUMELE CLIENTULUI"/>
            <TextField fx:id="clientName"/>
        </HBox>
        <Button text="ADAUGA" fx:id="addButton"/>
    </VBox>
</AnchorPane>
